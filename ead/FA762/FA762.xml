        <c id="aspace_4f091b02d3150946698f53eadd1a7531" level="file">
          <did>
            <unittitle>Ford Foundation Overseas Representatives' suggestions for Art Purchase Program</unittitle>
            <unitdate normal="1966/1966" type="inclusive">1966</unitdate>
            <container id="aspace_8638c30c03c8735a7dde63523c68700e" label="Mixed Materials" type="box">1</container>
